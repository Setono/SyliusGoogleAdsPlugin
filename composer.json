{
    "name": "setono/sylius-google-ads-plugin",
    "description": "Google Ads plugin for Sylius.",
    "license": "MIT",
    "type": "sylius-plugin",
    "keywords": [
        "sylius",
        "sylius-plugin"
    ],
    "require": {
        "php": ">=8.1",
        "brick/phonenumber": "^0.5.0",
        "doctrine/collections": "^1.6",
        "doctrine/orm": "^2.7",
        "doctrine/persistence": "^1.3 || ^2.2 || ^3.2",
        "google/auth": "^1.26",
        "google/gax": "^1.19",
        "googleads/google-ads-php": "^20.0",
        "knplabs/knp-menu": "^3.1",
        "ocramius/doctrine-batch-utils": "^2.4",
        "psr/event-dispatcher": "^1.0",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "setono/composite-compiler-pass": "^1.1",
        "setono/doctrine-object-manager-trait": "^1.0",
        "sylius/channel": "^1.0",
        "sylius/channel-bundle": "^1.0",
        "sylius/core": "^1.0",
        "sylius/core-bundle": "^1.0",
        "sylius/order": "^1.0",
        "sylius/resource-bundle": "^1.6",
        "sylius/ui-bundle": "^1.0",
        "symfony/config": "^5.4 || ^6.0",
        "symfony/console": "^5.4 || ^6.0",
        "symfony/dependency-injection": "^5.4 || ^6.0",
        "symfony/event-dispatcher": "^5.4 || ^6.0",
        "symfony/event-dispatcher-contracts": "^2.5 || ^3.3",
        "symfony/form": "^5.4 || ^6.0",
        "symfony/http-foundation": "^5.4 || ^6.0",
        "symfony/http-kernel": "^5.4 || ^6.0",
        "symfony/messenger": "^5.4 || ^6.0",
        "symfony/options-resolver": "^5.4 || ^6.0",
        "symfony/routing": "^5.4 || ^6.0",
        "symfony/string": "^5.4 || ^6.0",
        "symfony/validator": "^5.4 || ^6.0",
        "symfony/workflow": "^5.4 || ^6.0",
        "twig/twig": "^2.15 || ^3.4",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "api-platform/core": "^2.7.16",
        "babdev/pagerfanta-bundle": "^3.8",
        "behat/behat": "^3.13",
        "doctrine/doctrine-bundle": "^2.10",
        "jms/serializer-bundle": "^4.2",
        "lexik/jwt-authentication-bundle": "^2.17",
        "matthiasnoback/symfony-config-test": "^5.0",
        "phpunit/phpunit": "^9.6",
        "psalm/plugin-phpunit": "^0.18",
        "setono/code-quality-pack": "^2.5",
        "sylius/sylius": "~1.12.9",
        "symfony/debug-bundle": "^5.4 || ^6.0",
        "symfony/dotenv": "^5.4 || ^6.0",
        "symfony/intl": "^5.4 || ^6.0",
        "symfony/serializer": "^5.4 || ^6.0.1",
        "symfony/web-profiler-bundle": "^5.4 || ^6.0",
        "symfony/webpack-encore-bundle": "^1.17",
        "weirdan/doctrine-psalm-plugin": "^2.9",
        "willdurand/negotiation": "^3.1"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Setono\\SyliusGoogleAdsPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Setono\\SyliusGoogleAdsPlugin\\": "tests/"
        },
        "classmap": [
            "tests/Application/Kernel.php"
        ]
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "ergebnis/composer-normalize": true,
            "symfony/thanks": false
        },
        "sort-packages": true
    },
    "scripts": {
        "analyse": "psalm",
        "check-style": "ecs check",
        "fix-style": "ecs check --fix",
        "phpunit": "phpunit"
    }
}
